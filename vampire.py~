name = "Joe"
Age = 101
if name == "Alice":
    print("Hi Alice")
else:
    if Age < 12:
        print("Your are not Alice, Kiddo.")
    else:
        if Age > 2000:
            print("Unlike you, Alice is not and undead, immortal vampire.")
        else:
            if Age > 100:
                print("You are not Alice Grannie")
    print("Hi Stranger!")
